{
    "name": "node-typescript-api",
    "version": "1.0.0",
    "description": "Node Typescript Api",
    "main": "index.js",
    "repository": "https://github.com/ciceromngr/node-typescript-api.git",
    "author": "Cicero Romao <cicerotransformice@gmail.com>",
    "license": "MIT",
    "keywords": [
        "nodejs",
        "typescript",
        "api"
    ],
    "scripts": {
        "build": "tsc",
        "start": "yarn build && node dist/src/index.js",
        "dev": "tsnd src/index.ts",
        "lint": "eslint ./src ./test --ext .ts",
        "lint:fix": "yarn lint --fix",
        "jest:functional": "jest --projects ./test --runInBand",
        "style:check": "prettier --check src/**/*.ts test/**/*.ts",
        "style:fix": "prettier --write src/**/*.ts test/**/*.ts"
    },
    "dependencies": {
        "@overnightjs/core": "^1.7.6",
        "express": "^4.17.2",
        "module-alias": "^2.2.2"
    },
    "devDependencies": {
        "@types/express": "^4.17.13",
        "@types/jest": "^27.4.0",
        "@types/module-alias": "^2.0.1",
        "@types/node": "^17.0.16",
        "@types/supertest": "^2.0.11",
        "@typescript-eslint/eslint-plugin": "^5.11.0",
        "@typescript-eslint/parser": "^5.11.0",
        "eslint": "^8.8.0",
        "jest": "^27.5.1",
        "prettier": "^2.5.1",
        "supertest": "^6.2.2",
        "ts-jest": "^27.1.3",
        "ts-node-dev": "^1.1.8",
        "typescript": "^4.5.5"
    }
}